{
	"name": "website",
	"version": "0.0.1",
	"scripts": {
		"dev": "npm-run-all --parallel dev:vite dev:typesafe-i18n",
		"dev:vite": "vite dev --host",
		"dev:typesafe-i18n": "typesafe-i18n",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "pnpm prisma generate",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"typesafe-i18n": "typesafe-i18n"
	},
	"devDependencies": {
		"@prisma/client": "~4.7.1",
		"@sveltejs/adapter-node": "1.0.0-next.102",
		"@sveltejs/adapter-vercel": "1.0.0-next.84",
		"@sveltejs/kit": "1.0.0-next.584",
		"@tailwindcss/forms": "^0.5.3",
		"@types/cookie": "^0.5.1",
		"@types/workbox-build": "5.0.1",
		"@typescript-eslint/eslint-plugin": "^5.47.0",
		"@typescript-eslint/parser": "^5.47.0",
		"autoprefixer": "^10.4.13",
		"eslint": "^8.30.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"imagetools-core": "^3.2.3",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.20",
		"prettier": "^2.8.1",
		"prettier-plugin-svelte": "^2.9.0",
		"prettier-plugin-tailwindcss": "^0.2.1",
		"prisma": "^4.8.0",
		"react": "~18.2.0",
		"svelte": "^3.55.0",
		"svelte-check": "^2.10.3",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.2.4",
		"tslib": "^2.4.1",
		"typesafe-i18n": "^5.18.1",
		"typescript": "^4.9.4",
		"vite": "^4.0.3",
		"vite-imagetools": "^4.0.12",
		"workbox-window": "^6.5.4"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/material-icons": "^4.5.4",
		"@fontsource/material-icons-rounded": "^4.5.4",
		"@fontsource/outfit": "^4.5.10",
		"@supabase/supabase-js": "^2.2.2",
		"@vercel/analytics": "^0.1.6",
		"@vercel/edge": "^0.1.2",
		"cssnano": "^5.1.14",
		"svelte-inview": "^3.0.1",
		"web-vitals": "^3.1.0",
		"zod": "^3.20.2"
	}
}
